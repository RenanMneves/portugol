programa {
  funcao inicio() {
    cadeia nome
    cadeia disciplinas 
    real x
    real y 
    real media 
    escreva("nome do aluno","\n")
    leia(nome)
    escreva("escreva disciplinas","\n")
    leia(disciplinas)
    escreva("x ","\n")
    leia(x)
    escreva("y","\n")
    leia(y)
    media = ( x + y) /2 
    escreva("aluno", nome, "\n")
    escreva("disciplinas", disciplinas, "\n")
    escreva("media",media,"\n")
  }
}
